    <sql>select
 to_number(to_char(
to_date(p_date,'YYYY/MM/DD'),'YYYY')) p_year,
 to_number(to_char(
to_date(p_date,'YYYY/MM/DD'),'MM')) p_month,
 to_number(to_char(
to_date(p_date,'YYYY/MM/DD'),'DD')) p_day,
to_char(
to_date(p_date,'YYYY/MM/DD'),'Day') p_week,
p_date p_date,
service_name,
total_watch_time,
total_watch_time_hours,
online_max_time_stamp,
online_max,
bitrate_max,
ip_count,
avg_watch_time,
volume_mb,
access_count,
bitrate_max_time_stamp,
program_name,
program_type,
program_id,
decode(nvl(category_id,'unknow'),'OTHERS','HOUSE-VIDEO',nvl(category_id,'unknow')) category_id,
category_name,
decode(nvl(content_type,'unknow'),'others','house-video',nvl(content_type,'unknow')) content_type,
content_id,
content_title,
episode_no,
provider,
case
  when instr(service_name,'free') > 0 then
    '免費'
  else
    '付費'
  end 
free,
cat1,
activate_flg,
decode(service_name,'litvfreepc','web','litvfreemobile','mobile','litvfreepad','mobile','litvfreetv','tv','litvpc','web','litvmobile','mobile','litvpad','mobile','litvtv','tv',service_name) platform,
program_size,
nvl(ana_code1,'NULL') ana_code1,
nvl(cdn_provider,'HINET') cdn_provider,
case 
  when service_name like '%poc%' then
p_date||'poc'||program_id
  when service_name like '%vc%' then 
p_date||'vc'||program_id 
  else
   p_date||'lc'||program_id
end PLAYOUT_KEY,

to_number(time) p_time,
to_date(p_date,'YYYY/MM/DD') r_date,
P_DATE||' '||lpad(time,2,'0') p_date_hour
from cdn_programinfo_view_em_ch
where 
time &lt;&gt; 'day'
 --and program_id in ('4gtv-4gtv152','4gtv-4gtv072')
--  and p_date>='2018/12/01'
and p_date>=replace('${start_date}','-','/') 
and p_date&lt;=replace('${end_date}','-','/') 
-- and p_date >='2019/11/21' 
 --and p_date >= to_char(sysdate-30,'YYYY/MM/DD')
and decode(service_name,'litvfreepc','web','litvfreemobile','mobile','litvfreepad','mobile','litvfreetv','tv','litvpc','web','litvmobile','mobile','litvpad','mobile','litvtv','tv',service_name) &lt;&gt; 'embed'
and program_id is not null
and program_name is not null
and service_name is not null
and (access_count >0 or total_watch_time > 0)


 



</sql>
    <limit>0</limit>

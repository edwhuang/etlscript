var v_FIRST_BUY_NO = '';
var v_IN_SERVICE = IN_SERVICE;
if(FIRST_BUY_NO==null)
{v_FIRST_BUY_NO='非首次';}else
{v_FIRST_BUY_NO='首次';}
if( PAY_TYPE=='cns' ){ v_FIRST_BUY_NO='首次';}
if( PAY_TYPE=='IOS二次扣款' | PAY_TYPE=='信用卡二次扣款') {
 v_FIRST_BUY_NO='續扣';
v_IN_SERVICE = null;
}
</jsScript_script>

	(timestamp '1970-01-01 00:00:00 GMT' + numtodsinterval(?/1000, 'SECOND')) /* PURCHASE_DATE */
    , '?'
    , 'WD0005'/* PACKAGE_ID */
    , 'A' /* STATUS_FLG */
    , SYSDATE /* CREATE_DATE */
    , null /* SERVICE_ACT_DATE */ 

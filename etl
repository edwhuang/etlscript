    <connection>twbidb_125</connection>
    <sql>Select a.cdn_provider,
to_date(a.p_date,'YYYY/MM/DD') "日期",
replace(a.p_date,'/','-') pds,
to_char(to_date(a.p_date,'YYYY/MM/DD'),'YYYY-MM') "年月",
       to_char(to_date(a.p_date,'YYYY/MM/DD'),'day') 星期,
       c.cat1 platform1,
       c.cal_type,
       case when c.service_type = 'Vod and Live Channel' then
         'Live Channel'
            when  c.service_type = 'Vod and Live Channel' then
            'Vod'
            else
               c.service_type
            end service_type
         ,
       c.service_name,
       c.cat1,
       d.vod_channel_id,
       d.cdn_code,
       d.channel_name,
       f.content_title,
       d.title,
       c.platform,
       a.content_type,
       a.access_count,
       a.total_watch_time ,
       a.total_watch_time / 60 total_watch_time_hours,
       a.total_watch_time/access_count AVG_WATCH_TIME,
       a.ISP ISP廠商,
       e.ref2 user_agent_分類,
       e.user_agent user_agent,
       a.playout_key,
       f.episode_no,
	   d.channel_type
  from pentaho.ans_cdn_daily_report a,
       pentaho.ans_cdn_service_name c,
       pentaho.ans_channel_playout d,
       iptv.content_meta f,
       pentaho.user_agent_desc e
 where
       c.service_name = a.service_name
   and a.access_count > 0
   and a.total_watch_time > 0
   and e.user_agent (+)=a.user_agent
   and d.air_date (+)= to_date(a.p_date,'YYYY/MM/DD')
   and f.program_id (+)= a.program_id
   and a.playout_key=d.main_key (+)
   and NVL(e.ref2,'a') in ('TBC','CNS','新彰')
    and (a.service_name like '%vc%' or a.service_name like '%poc%')
  and  c.service_type &lt;&gt; 'CDN_Erase'
     AND a.p_date >=replace('${start_date}','-','/')
   and a.p_date &lt;= replace('${end_date}','-','/')
union all
Select a.cdn_provider,
to_date(a.p_date,'YYYY/MM/DD') "日期",
replace(a.p_date,'/','-') pds,
to_char(to_date(a.p_date,'YYYY/MM/DD'),'YYYY-MM') "年月",
       to_char(to_date(a.p_date,'YYYY/MM/DD'),'day') 星期,
      c.cat1 platform1,
       c.cal_type,
       case when c.service_type = 'Vod and Live Channel' then
         'Live Channel'
            when  c.service_type = 'Vod and Live Channel' then
            'Vod'
            else
               c.service_type
            end service_type
         ,
       c.service_name,
       c.cat1,
       d.vod_channel_id,
       d.cdn_code,
       d.channel_name,
       f.content_title,
       d.title,
       c.platform,
       a.content_type,
       a.access_count,
       a.total_watch_time ,
       a.total_watch_time / 60 total_watch_time_hours,
       a.total_watch_time/access_count AVG_WATCH_TIME,
       a.ISP ISP廠商,
       e.ref2 user_agent_分類,
       e.user_agent user_agent,
       a.playout_key,
       f.episode_no,
		d.channel_type
  from pentaho.ans_cdn_daily_report a,
       pentaho.ans_cdn_service_name c,
       pentaho.ans_channel_playout d,
       iptv.content_meta f,
       pentaho.user_agent_desc e
 where
       c.service_name = a.service_name
   and a.access_count > 0
   and a.total_watch_time > 0
   and e.user_agent (+)=a.user_agent
   and d.air_date (+)= to_date(a.p_date,'YYYY/MM/DD')
   and f.program_id (+)= a.program_id
      and a.playout_key=d.main_key
      and NVL(e.ref2,'a') not in ('TBC','CNS','新彰')
    AND a.p_date >=replace('${start_date}','-','/')
   and a.p_date &lt;= replace('${end_date}','-','/')</sql>

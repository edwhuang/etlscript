        <jsScript_type>0</jsScript_type>
        <jsScript_name>Script 1</jsScript_name>
        <jsScript_script>var p_platform;

p_platform=platform;

if(platform == '|web') {

p_platform='web';
}

if(platform == '|m-web'){
p_platform='m-web';
}

if(platform == '|tv'){
p_platform='tv';
}

if(indexOf(ad_clip,'IOS_') >= 0||indexOf(platform,'iPhone') >= 0||indexOf(platform,'iPad') >= 0)
{
  p_platform='ios';
  ad_clip =replace(ad_clip,'IOS_','');
}



var ad_type="";
var p_ad_clip = ad_clip;

if(indexOf(ad_clip,'|')>0)
{
	ad_type = substr(p_ad_clip,0,indexOf(ad_clip,'|'));
 	p_ad_clip = substr(p_ad_clip,indexOf(ad_clip,'|')+1);
    
    if(ad_id=='ad-video'){ ad_clip=ad_type;ad_type = null
}
   else 
   {
   // if(p_ad_clip=='303'||p_ad_clip=='yahoo') { p_ad_clip=ad_type; ad_type = null;}

  //  if(substr(ad_type,0,1) == '3' ||substr(ad_type,0,1) == '7'||p_ad_clip=='303'||p_ad_clip=='yahoo'||substr(ad_type,0,2) == 'hi' ){ p_ad_clip=ad_type; ad_type = null;}
    if(content_type == null){
    content_type = substr(p_ad_clip,0,indexOf(p_ad_clip,'|'));}
    p_ad_clip = substr(p_ad_clip,indexOf(p_ad_clip,'|')+1);
    ad_id=substr(p_ad_clip,0,indexOf(p_ad_clip,'|'));
    if(ad_id == "logo"){ ad_id="ad_logo";}
    else if (ad_id=="noad") { ad_id="ad_noad";}
    else if (ad_id=="pausebanner") { ad_id="ad_p_b";}
    
    p_ad_clip = substr(p_ad_clip,indexOf(p_ad_clip,'|')+1);
    if(indexOf(p_ad_clip,'|')>0){
     p_ad_clip = substr(p_ad_clip,0,indexOf(p_ad_clip,'|'))
}
   // ;
    ad_clip=p_ad_clip;
}

}

if(p_ad_clip == '793604647')
{
    p_platform='embed';
}

if(p_ad_clip == '7936064638')
{
    p_platform='embed';
}

if(p_ad_clip == '793606592')
{
    p_platform='embed';
}
if(p_ad_clip == '3872969')
{
    platform='tv';
    ad_clip = '3872969';
}
if(substr(p_ad_clip,0,2) == 'hi')
{
    ad_id='ad-video';
}

if(PROJECT=='hinet-embed'|indexOf(ad_clip,'hiemb')>=0)
{
  p_platform='hinet-embed';
}

if(content_type == null) {content_type="unknow";}
if(content_title== null) {content_title="unknow";}

var p_date_str="";
p_date_str=substr(p_date,0,4)+"/"+substr(p_date,4,2)+"/"+substr(p_date,6,2);
</jsScript_script>
      </jsScript>

    <sql> select channel_number,cdn_code,channel_name,program_index,title,channel_type from (
SELECT
 b.channel_number,a.cdn_code,a.channel_name,program_index,title,b.report_type as channel_type
FROM litv_cms_report.vod_channel_program_list a
join litv_cms_report.litv_channel_list b
on a.cdn_code=b.cdn_code
WHERE a.channel_type='VOD CHANNEL'
 collate utf8mb4_general_ci
and b.report_type='LITV_FREE' 
 collate utf8mb4_general_ci
group by b.channel_number,a.cdn_code,a.channel_name,program_index,title,b.report_type 
) t
order by channel_number desc



</sql>

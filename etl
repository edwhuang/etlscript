  <step>
    <name>當日取消自動扣款</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <connection>ClickHousre</connection>
    <sql>SELECT 
STOP_RECURRENT_DATE PURCHASE_DATE,PACKAGE_CAT_ID1,'自動扣款' FIRST_BUY_NO,'停止' IN_SERVICE,'停止' ANS_CODE1,PAY_TYPE,SOFTWARE_GROUP,COUNT(*) P_COUNT
FROM rdbms.temp_purchase_dtls
WHERE PACKAGE_CAT_ID1 in ('ALL','CHANNEL_A','VOD_CHANNEL_DELUX')
AND PAY_TYPE in ('信用卡','便利商店繳款','ATM','信用卡二次扣款','IOS二次扣款')
and RECURRENT='R'
and CURRENT_RECURRENT_S='B'
GROUP BY STOP_RECURRENT_DATE,PACKAGE_CAT_ID1,PAY_TYPE,SOFTWARE_GROUP
ORDER BY STOP_RECURRENT_DATE,PACKAGE_CAT_ID1</sql>
    <limit>0</limit>

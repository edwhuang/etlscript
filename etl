    </partitioning>
    <connection>twdevdb</connection>
    <sql>SELECT D.SERIAL_ID , B.MOBILENUMBER, NVL(B.EMAILADDRESS, B.UNCONFIRMEDEMAILADDRESS) EMAIL
, D.PACKAGE_ID, D.PACKAGE_CAT1
, M.MAS_NO, M.ORG_MAS_NO, M.LAST_FAIL_DATE  
FROM 
(SELECT A.PK_NO, A.MAS_NO, SUBSTR(A.SRC_NO,3,17) ORG_MAS_NO, A.MAS_DATE LAST_FAIL_DATE       -- 最近一次重複扣款失敗
FROM IPTV.BSM_PURCHASE_MAS A
WHERE A.STATUS_FLG = 'F'
AND (SUBSTR(A.SRC_NO,3,17), A.MAS_DATE) IN 
(SELECT SUBSTR(R.SRC_NO,3,17) ORG_MAS_NO, MAX(R.MAS_DATE) 
FROM IPTV.BSM_PURCHASE_MAS R
WHERE R.STATUS_FLG = 'F'
AND R.MAS_DATE >= TRUNC(SYSDATE) - 2
AND R.SRC_NO LIKE 'REPUR%'
GROUP BY SUBSTR(R.SRC_NO,3,17)
HAVING COUNT(*) >=3)        -- 已失敗三次
) M,
IPTV.BSM_CLIENT_DETAILS D
,IPTV.WEB_REGISTERED B
WHERE M.ORG_MAS_NO = D.SRC_NO
AND D.SERIAL_ID = B.ACCOUNTID
</sql>

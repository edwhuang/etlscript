    <sql>SELECT
 SORTURL,
  REPLACE(replace(SORTURL,'https','http'),'http://goo.gl/','') SHORT,
  replace(replace('${date}','/','-'),'/','-') p_date
FROM GA_SHORTURL_SETTING

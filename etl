    <sql>SELECT
  program_id
, series_id as SERIAL_ID
, series_id as SERIAL_NO
, cms_content_id as CONTENT_ID
, publisher
, series_id
, season as SEASON_NO
,substring(ep_number,3,6) as EPISODE_NO
,ep_number
, vod_type
, enabled
, trim(title) as CONTENT_TITLE
, trim(subtitle) as subtitle
, engtitle
, recommend
,origincountry
,CONCAT( provider_id) as provider_id
, date_format(start_authorized_date,'%Y/%m/%d') as cp_start
, date_format(end_authorized_date,'%Y/%m/%d') as cp_end
,null as CAT1
,null as SECONDARY_MARK
,null as PRONUNCIATION
,upper(v.name) as CATEGORY_ID_V
,v.name as CONTENT_TYPE
FROM vod_program p
left join vod_type v
on v.id=p.vod_type 






</sql>

    </partitioning>
    <connection>twdevdb</connection>
    <sql>select 
G.PAY_SUPPLY_ID "廠商",
G.PAY_TYPE "購買方式", 
G.OWNER_PHONE "用戶帳號", 
G.COUPON_ID "COUPON ID",
G.PACKAGE_CAT1 "套餐名稱",
G.CUP_PROGRAM_ID "coupon專案代號",
G."NAME" "coupon專案名稱", 
TO_CHAR(G.REAL_START_DATE, 'YYYY/MM/DD') 服務開始日,
TO_CHAR(G.REAL_END_DATE, 'YYYY/MM/DD') 服務到期日
from
(select             -- 用戶/coupon/服務起始日集合 清單
P.PAY_SUPPLY_ID, 
A.PAY_TYPE, 
C.SERIAL_ID,
C.OWNER_PHONE, 
R.COUPON_ID,
D.PACKAGE_CAT1,
P.CUP_PROGRAM_ID,
P."NAME", 
MIN(F.START_DATE) REAL_START_DATE,
MAX(F.END_DATE) REAL_END_DATE
from IPTV.BSM_PURCHASE_MAS a,
IPTV.BSM_PURCHASE_ITEM b,
IPTV.BSM_PACKAGE_MAS d,
IPTV.BSM_CLIENT_DETAILS f,
IPTV.BSM_COUPON_MAS r,
IPTV.BSM_COUPON_PROG_MAS p,
IPTV.BSM_CLIENT_MAS c
where A.PK_NO = B.MAS_PK_NO
and B.PACKAGE_ID = D.PACKAGE_ID
and B.PK_NO = F.SRC_ITEM_PK_NO
and A.SRC_NO = R.MAS_NO
and R.PROGRAM_ID = P.CUP_PROGRAM_ID
and A.SERIAL_ID = c.SERIAL_ID
and A.STATUS_FLG = 'Z'
and F.STATUS_FLG = 'P'
and P.PAY_SUPPLY_ID like '台數科%'
and (A.PAY_TYPE LIKE '兌換券%' OR (A.PAY_TYPE = '贈送' AND D.PACKAGE_CAT1 IN ('199超值組合','豪華組合餐')))
group by
P.PAY_SUPPLY_ID, 
A.PAY_TYPE, 
C.SERIAL_ID,
C.OWNER_PHONE, 
R.COUPON_ID,
D.PACKAGE_CAT1,
P.CUP_PROGRAM_ID,
P."NAME") G,
(select             -- 過濾掉已延展服務的用戶
R.SERIAL_ID
from IPTV.BSM_PURCHASE_MAS a,
IPTV.BSM_PURCHASE_ITEM b,
IPTV.BSM_PACKAGE_MAS d,
IPTV.BSM_CLIENT_DETAILS f,
IPTV.BSM_COUPON_MAS r,
IPTV.BSM_COUPON_PROG_MAS p
where A.PK_NO = B.MAS_PK_NO
and B.PACKAGE_ID = D.PACKAGE_ID
and B.PK_NO = F.SRC_ITEM_PK_NO
and A.SRC_NO = R.MAS_NO
and R.PROGRAM_ID = P.CUP_PROGRAM_ID
and A.STATUS_FLG = 'Z'
and F.STATUS_FLG = 'P'
and P.AUTO_EXTEN_SERVICE = 'N'
and P.PAY_SUPPLY_ID like '台數科%'
and (A.PAY_TYPE LIKE '兌換券%' OR (A.PAY_TYPE = '贈送' AND D.PACKAGE_CAT1 IN ('199超值組合','豪華組合餐')))
group by R.SERIAL_ID
having TRUNC(MAX(F.END_DATE)) >= ADD_MONTHS(trunc(sysdate,'MM'),1)
and TRUNC(MAX(F.END_DATE)) &lt; ADD_MONTHS(trunc(sysdate,'MM'),2)) H
where G.SERIAL_ID = H.SERIAL_ID
and TRUNC(G.REAL_END_DATE) >= ADD_MONTHS(trunc(sysdate,'MM'),1)     -- 濾出即將到期的coupon
and TRUNC(G.REAL_END_DATE) &lt; ADD_MONTHS(trunc(sysdate,'MM'),2)
order by G.PAY_TYPE DESC, G.OWNER_PHONE, G.REAL_END_DATE desc, G.PACKAGE_CAT1, G."NAME"</sql>
    <limit>0</limit>

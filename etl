      <schema_name/>
    </partitioning>
    <connection>twdevdb</connection>
    <sql>SELECT
  CATEGORY_ID
, CATEGORY_NAME
, CONTENT_TYPE
, CONTENT_ID
, CONTENT_TITLE
, EPISODE_NO
, SEASON_NO
, decode(content_type,'others','LiTV',PROVIDER) PROVIDER
, SERIAL_ID
, SERIAL_NO
, SECONDARY_MARK
, PRONUNCIATION
, COUNTRIES
, COUNTRIES_EN
, NVL(PROGRAM_ID,CONTENT_ID) PROGRAM_ID
, VIDEO_TYPE
, max(update_date) last_update
, '' CAT1
, active_flg

FROM CCC_PROGRAM_ASSET
where content_type not in ('channel')
group by   CATEGORY_ID
, CATEGORY_NAME
, CONTENT_TYPE
, CONTENT_ID
, CONTENT_TITLE
, EPISODE_NO
, SEASON_NO
, PROVIDER
, SERIAL_ID
, SERIAL_NO
, SECONDARY_MARK
, PRONUNCIATION
, COUNTRIES
, COUNTRIES_EN
, NVL(PROGRAM_ID,CONTENT_ID)
, VIDEO_TYPE
, active_flg

order by Content_id


</sql>

        <jsScript_name>Script 1</jsScript_name>
        <jsScript_script>//Script here

var _purchase_no;
if( PURCHASE_NO_1 != null)
{
   _purchase_no=PURCHASE_NO_1;
}
else if(PURCHASE_NO != null)
{
   _purchase_no = PURCHASE_NO;
}
else 
{
   _purchase_no = PURCHASE_NO_3;
}

</jsScript_script>
      </jsScript>
    </jsScripts>

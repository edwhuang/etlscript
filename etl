    <connection>twbidb_125</connection>
    <sql>SELECT nvl(M.ACCESS_COUNT,0) ACCESS_COUNT,nvl(M.TOTAL_WATCH_TIME,0) TOTAL_WATCH_TIME,NVL(M.VOLUME_MB,0) VOLUME_MB,
 D.P_YEAR,D.P_MONTH,D.P_DAY,D.P_DATE_S,D.P_YYYYMM,D.WEEK_DATE,
 M.PROGRAM_ID,
 nvl(c.content_type,'unknow') content_type,
 nvl(decode(c.content_type,'ent','Yes娛樂',c.content_title),'unknow') content_title,
 nvl(c.episode_no,0) episode_no,
 nvl(c.provider,'unknow') provider,
 nvl(c.country_code,'unknow') country_code,
 s.service_name,
 s.cal_type,
 s.platform,
 s.service_type,
 m.cdn_provider,
 nvl(p.cdn_code,'unknow') cdn_code,
 nvl(p.channel_name,'unknow') channel_name,
 nvl(p.title,'unknow') ch_prog_title,
 nvl(p.ep_number,0) ep_number,
 nvl(p.vod_channel_id,0) channel_id,
M.ISP,
M.USER_AGENT,
ag.REF2 agent_desc,
c.ans_code1,
c.provider

 FROM ANS_CDN_DAILY_REPORT M
 join PENTAHO.CALENDER D
 ON D.P_DATE_S=M.P_DATE
 left join iptv.content_meta c
 on c.program_id=m.program_id
 join ans_cdn_service_name s
 on s.service_name=m.service_name
 left join ANS_CHANNEL_PLAYOUT p
 on p.main_key=m.playout_key
 left join USER_AGENT_DESC ag
on ag.user_agent=m.user_agent 
where p_date_s >= replace('${start_date}','-','/')
and p_date_s &lt;=  replace('${end_date}','-','/')
order by m.p_date,service_name,program_id
 </sql>
    <limit>0</limit>
    <lookup/>
    <execute_each_row>N</execute_each_row>

  <step>
    <name>Table input</name>
    <type>TableInput</type>
    <description/>
    <distribute>N</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <connection>ReportDB125</connection>
    <sql>select a.DEVICE_ID device_id, b.ACCOUNT_ID account_id, b.PHONE_NO phone_no
from 
(SELECT c.DEVICE_ID, c.CLIENT_ID
FROM PENTAHO.ANS_CLIENT_DEVICE_DETAIL c,
(SELECT DEVICE_ID, MAX(activate_date) max_activate_date
FROM PENTAHO.ANS_CLIENT_DEVICE_DETAIL
GROUP BY DEVICE_ID) d
WHERE c.DEVICE_ID = d.DEVICE_ID
AND c.activate_date = d.max_activate_date
) a,
(SELECT g.ACCOUNT_ID , g.PHONE_NO
FROM PENTAHO.ACCOUNT_INFO g,
(SELECT ACCOUNT_ID, MAX(ACCOUNT_CREATE) MAX_ACCOUNT_CREATE
FROM PENTAHO.ACCOUNT_INFO
GROUP BY ACCOUNT_ID) h
WHERE g.ACCOUNT_ID = h.ACCOUNT_ID
AND g.ACCOUNT_CREATE = h.MAX_ACCOUNT_CREATE
) b
where a.CLIENT_ID = b.ACCOUNT_ID
and a.DEVICE_ID &lt;&gt; 'null'
and a.DEVICE_ID &lt;&gt; '000000000000'</sql>
    <limit>0</limit>

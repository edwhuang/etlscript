  <step>
    <name>modify_platform 對應</name>
    <type>ScriptValueMod</type>
    <description/>
    <distribute>N</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <compatible>N</compatible>
    <optimizationLevel>9</optimizationLevel>
    <jsScripts>
      <jsScript>
        <jsScript_type>0</jsScript_type>
        <jsScript_name>Script 1</jsScript_name>
        <jsScript_script>//Script here

var ad_clip2=ad_clip;
if(platform =="|web"){var platform_2 = 'web';}
else if (platform =="|m-web"){var platform_2 = 'm-web';}
else if(!(platform =="web" || platform=="m-web" || platform=="tv"))
{var platform_2 = 'mobile';}
else
{var platform_2= platform;}
if(indexOf(ad_clip,'IOS_') >= 0||indexOf(platform,'iPhone') >= 0||indexOf(platform,'iPad') >= 0)
{
  platform_2='ios';
  ad_clip2 =replace(ad_clip,'IOS_','');
}


if(ad_clip == '793604647')
{
    platform_2='embed';
}

if(ad_clip == '7936064638')
{
    platform_2='embed';
}

if(ad_clip == '793606592')
{
    platform_2='embed';
}

var program_id="";
if (indexOf(asset_id,'P') >= 0)
{
	program_id = substr(asset_id,0,indexOf(asset_id,'P'));
}
else if (indexOf(asset_id,'P') >= 0)
{
	program_id = substr(asset_id,0,indexOf(asset_id,'M'));
}
else if (indexOf(asset_id,'-',indexOf(asset_id,'-')+1) >= 0)
{
   program_id = substr(asset_id,0,indexOf(asset_id,'-',indexOf(asset_id,'-')+1));
}
else
{
   program_id = asset_id;
}

var p_date="";
var p_time="";

p_date=date2str(local_time,"yyyy/MM/dd");
p_time=date2str(local_time,"kk");
if(p_time=="24"){
  p_time="00";
}





</jsScript_script>
      </jsScript>
    </jsScripts>

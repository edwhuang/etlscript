    <sql>Select a.cdn_provider,
replace(a.p_date,'/','-') P_DATE,
    
       to_char(to_date(a.p_date,'YYYY/MM/DD'),'day') P_WEEK,
       c.cal_type,
       c.service_type,
       c.cat1 platform1,
       d.category_id,
       d.content_type,
       d.provider,
       d.program_id,
       d.title,
       d.content_id,
       d.country_code,
       d.episode_no,
       a.service_name,
       a.platform,
       a.access_count,
       a.volume_mb,
       a.total_watch_time,
       a.total_watch_time / 60 TOTAL_WATCH_TIME_HOURS,
       a.total_watch_time/access_count AVG_WATCH_TIME,
       a.ISP ISP_PROVIDER ,
       e.ref2 user_agent_TYPE,
       e.user_agent user_agent
  from pentaho.ans_cdn_daily_report a,
       pentaho.ans_cdn_service_name c,
       iptv.content_meta d,
       pentaho.user_agent_desc e
 where 
       c.service_name = a.service_name
   and a.access_count > 0
   and a.total_watch_time > 0
   and c.service_type not in ( 'Vod Channel','Playout Channel')
   and a.program_id=d.program_id(+)
   and d.content_type(+) &lt;&gt;'channel'
  and  c.service_type &lt;&gt; 'CDN_Erase'
  and e.user_agent (+) = a.user_agent
   AND a.p_date >=replace('${start_date}','-','/')
   and a.p_date &lt;= replace('${end_date}','-','/')
 </sql>

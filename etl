       to_char(perday, 'YYYY-MM') p_yyyymm,
       to_char(perday-to_number(decode(to_char(perday,'d'),'2','1','3','2','4','3','5','4','6','5','7','6','1','7')),'yyyy-WW') week_of_year,
       to_char(perday-to_number(decode(to_char(perday,'d'),'2','1','3','2','4','3','5','4','6','5','7','6','1','7'))+1,'yyyy/MM/DD')||'-'||
       to_char(perday-to_number(decode(to_char(perday,'d'),'2','1','3','2','4','3','5','4','6','5','7','6','1','7'))+7,'yyyy/MM/DD') week_date,
     to_char(perday, 'YYYY-MM-DD') p_date_s2
  from (SELECT trunc(sysdate - 72) + level AS perDay --排除今日
          FROM dual
        CONNECT BY level &lt;= 180)</sql>

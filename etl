    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <connection>twbidb_iptv</connection>
    <sql>select
 to_number(to_char(
to_date(p_date,'YYYY/MM/DD'),'YYYY')) p_year,
 to_number(to_char(
to_date(p_date,'YYYY/MM/DD'),'MM')) p_month,
 to_number(to_char(
to_date(p_date,'YYYY/MM/DD'),'DD')) p_day,
to_char(
to_date(p_date,'YYYY/MM/DD'),'Day') p_week,

p_date,
service_name,
total_watch_time,
total_watch_time_hours,
online_max_time_stamp,
online_max,
bitrate_max,
ip_count,
avg_watch_time,
volume_mb,
access_count,
bitrate_max_time_stamp,
program_name,
program_type,
program_id,
decode(nvl(category_id,'unknow'),'OTHERS','HOUSE-VIDEO',nvl(category_id,'unknow')) category_id,
category_name,
decode(nvl(content_type,'unknow'),'others','house-video',nvl(content_type,'unknow')) content_type,
content_id,
content_title,
episode_no,
provider,
case
  when instr(service_name,'free') > 0 then
    '免費'
  else
    '付費'
  end 
free,
cat1,
activate_flg,
decode(service_name,'litvfreepc','web','litvfreemobile','mobile','litvfreepad','mobile','litvfreetv','tv','litvpc','web','litvmobile','mobile','litvpad','mobile','litvtv','tv',service_name) platform,
program_size,
nvl(ana_code1,'NULL') ana_code1,
nvl(cdn_provider,'HINET') cdn_provider,
case 
   when service_name like '%poc%' and (user_agent like '%TBC%' or user_agent like '%CNS%' or user_agent like '%CABLE%' or user_agent like '%HaTV%' )  then
p_date||'poccb'||program_id
  when service_name like '%poc%'  then
p_date||'poc'||program_id
    when service_name like '%vc%' and (user_agent like '%TBC%' or user_agent like '%CNS%' or user_agent like '%CABLE%' or user_agent like '%HaTV%') and  service_name like '%free%' then 
p_date||'vccbf'||program_id 
    when service_name like '%vc%' and (user_agent like '%TBC%' or user_agent like '%CNS%' or user_agent like '%CABLE%' or user_agent like '%HaTV%') then 
p_date||'vccb'||program_id 
when service_name like '%vc%' and  (service_name like '%player%' or service_name like '%emb%' or service_name like '%cmb%' or service_name like '%epfree%' or service_name like 'litvpcfreevcltnep' )  then 
p_date||'vcpf'||program_id 
  when service_name like '%vc%' and  service_name like '%free%'  then 
p_date||'vcf'||program_id 

  when service_name like '%vc%'  then 
p_date||'vc'||program_id 
  else
   p_date||'lc'||program_id
end PLAYOUT_KEY,
nvl(isp,'NULL') ISP,
nvl(user_agent,'NULL') USER_AGENT
from iptv.cdn_programinfo_view_em
where 
time='day'
--and user_agent like '%TBC%'
--and p_date='2021/10/07'
--and cdn_provider='AWS'
--and service_name like '%pc%'
--and program_type='LIVE'
and p_date>=to_char(to_date(replace('${start_date}','-','/'),'YYYY/MM/DD') - 3,'YYYY/MM/DD')

--and p_date not in ('2020/05/02')
--and  p_date >= '2021/08/23' and p_date &lt;= '2021/08/29'
and program_id is not null
and program_name is not null
and service_name is not null			


 



</sql>
    <limit>0</limit>

    <connection>twbidb125_pentaho</connection>
    <sql>select * from (
Select min(p_date) "Date Start",max(p_date) "Date End",a.content_type "Type",a.content_title "Title",sum(b.access_count) "Access Count",replace(substr(a.program_id,1,instr(a.program_id,'-')-1),'vod','') "Series No" from iptv.content_meta a ,pentaho.ans_cdn_daily_report b,pentaho.ans_cdn_service_name c
where b.program_id=a.program_id
and b.p_date >= to_char((to_date('20171201','YYYYMMDD')+trunc((sysdate-to_date('20171201','YYYYMMDD'))/7)*7)-7,'YYYY/MM/DD')
and b.p_date &lt; to_char((to_date('20171201','YYYYMMDD')+trunc((sysdate-to_date('20171201','YYYYMMDD'))/7)*7),'YYYY/MM/DD')
and c.service_name = b.service_name
and c.cal_type = '免費'
and b.service_name in ('litvfreepc','litvfreemobile')
and a.content_type in ( 'drama')
group by a.content_type,a.content_title,replace(substr(a.program_id,1,instr(a.program_id,'-')-1),'vod','')
order by "Access Count" desc
) where rownum &lt;=30
union all
select * from (
Select min(p_date) date_star,max(p_date) date_end,a.content_type,a.content_title,sum(b.access_count) access_count,replace(substr(a.program_id,1,instr(a.program_id,'-')-1),'vod','') series_no from iptv.content_meta a ,pentaho.ans_cdn_daily_report b,pentaho.ans_cdn_service_name c
where b.program_id=a.program_id
and b.p_date >= to_char((to_date('20171201','YYYYMMDD')+trunc((sysdate-to_date('20171201','YYYYMMDD'))/7)*7)-7,'YYYY/MM/DD')
and b.p_date &lt; to_char((to_date('20171201','YYYYMMDD')+trunc((sysdate-to_date('20171201','YYYYMMDD'))/7)*7),'YYYY/MM/DD')
and c.service_name = b.service_name
and c.cal_type = '免費'
and b.service_name in ('litvfreepc','litvfreemobile')
and a.content_type in ( 'show')
group by a.content_type,a.content_title,replace(substr(a.program_id,1,instr(a.program_id,'-')-1),'vod','')
order by access_count desc
) where rownum &lt;=30

</sql>

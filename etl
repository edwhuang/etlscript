  <step>
    <name>Filter 4gtv</name>
    <type>FilterRows</type>
    <description/>
    <distribute>Y</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <send_true_to>modify_platform 對應</send_true_to>
    <send_false_to>modify_platform 對應 2</send_false_to>
    <compare>
      <condition>

var p_ad_id='';
if(ad_id == 'ad_p_b')
{
  if(indexOf(ad_clip,'logo') >= 0 ||indexOf(ad_clip,'vod') >= 0 || indexOf(ad_clip,'noad') >= 0 || indexOf(ad_clip,'_ts') >= 0|| indexOf(ad_clip,'_TS') >= 0)
{p_ad_id='ad_other';}
else
{p_ad_id= 'ad_p_b';}
}
else
{ p_ad_id=ad_id;}


//Script here

var ad_clip2=ad_clip;
var slot="";
var slots="";
var ad_seq="F";

if(indexOf(ad_clip2,'==') >= 0)
{
  slot = substr(ad_clip2,indexOf(ad_clip2,'=='),indexOf(ad_clip2,'==',indexOf(ad_clip2,'==')+2)-indexOf(ad_clip2,'==')+2);
  ad_clip2=replace(ad_clip2,slot,'');
  slot = replace(slot,'==','');
  if(indexOf(slot,'/') >=0){
  slots = substr(slot,indexOf(slot,'/')+1);
  slot = substr(slot,0,indexOf(slot,'/'));
  if(slot == '1'){ad_seq='F';}
  else if (slot == slots) {ad_seq='L';}
  else {ad_seq='M'};
  
}
}



if(platform =="|web"){var platform_2 = 'web';}
else if (platform =="|m-web"){var platform_2 = 'm-web';}
else if (indexOf(platform,'|tv') >=0 ){platform_2 = 'tv';}
else if(!(platform =="web" || platform=="m-web" || platform=="tv"))
{platform_2 = 'mobile';}
else
{platform_2= platform;}

if(indexOf(ad_clip2,'IOS_') >= 0||indexOf(platform,'iPhone') >= 0||indexOf(platform,'iPad') >= 0)
{
  platform_2='ios';
 if(indexOf(ad_clip2,'IOS_') >= 0){
  ad_clip2 =replace(ad_clip2,'IOS_','');
  if(indexOf(ad_clip2,'|')>=0){
      ad_clip2 =substr(ad_clip2,0,indexOf(ad_clip2,'|'));
   }
} 
}

if(ad_clip2 == '793604647')
{
    platform_2='embed';
}

if(ad_clip2 == '7936064638')
{
    platform_2='embed';
}

if(ad_clip2 == '793606592')
{
    platform_2='embed';
}

ad_clip2=replace(ad_clip2,'VOD-CHANNEL-','');
ad_clip2=replace(ad_clip2,'top20-','');

if(ad_clip2 == '3872969')
{
    platform_2='tv';
    ad_clip2 = '3872969';
}

if(indexOf(ad_clip2,'3') == 0||indexOf(ad_clip2,'7')==0)
{p_ad_id='ad-video';}
if(indexOf(ad_clip2,'hiembed') >= 0)
{ p_ad_id='ad-video'
  if(indexOf(ad_clip2,'|') > 0){
  ad_clip2 = substr(ad_clip2,0,indexOf(ad_clip2,'|')+1);
}
}

var program_id="";
if (indexOf(asset_id,'P') >= 0)
{
	program_id = substr(asset_id,0,indexOf(asset_id,'P'));
}
else if (indexOf(asset_id,'M') >= 0)
{
	program_id = substr(asset_id,0,indexOf(asset_id,'M'));
}
else if (indexOf(asset_id,'-',indexOf(asset_id,'-')+1) >= 0)
{
   program_id = substr(asset_id,0,indexOf(asset_id,'-',indexOf(asset_id,'-')+1));
}
else
{
   program_id = asset_id;
}

var p_date="";
var p_time="";

p_date=date2str(local_time,"yyyy/MM/dd");
p_time=date2str(local_time,"kk");
if(p_time=="24"){
  p_time="00";
}








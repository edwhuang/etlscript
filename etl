  <step>
    <name>Row_import</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <connection>twbidb_pentaho</connection>
    <sql>select x.p_localdate,case when substr(ad_clip,1,1) in ('3','7') then 'ad-video' else ad_id end "ad_id",
 ad_clip "ad_clip",cdata "cdata",platform "platform",count(*) "cnt",x.p_localhour,
floor(to_number(to_char(x.local_time,'MI'))/6) period,
case when instr(x.program_id,'M') > 0 then substr(x.program_id,1,instr(x.program_id,'M')-1) else x.program_id end program_id
              from pentaho.row_ad3 x
             where 1 = 1
               AND p_localdate ='2016/12/28'
            --   AND x.local_time >= to_date(to_char(sysdate-(2/24),'YYYY/MM/DD HH24:')||'00:00','YYYY/MM/DD HH24:MI:SS')
               
               and cdata in ('serve','parserError','request','blocked','noAd','ads-start','impression')
            group by  x.p_localdate,ad_id,ad_clip,cdata,platform,content_type,content_title, x.p_localhour,x.program_id,floor(to_number(to_char(x.local_time,'MI'))/6)</sql>
    <limit>0</limit>
    <lookup/>
    <execute_each_row>N</execute_each_row>
    <variables_active>N</variables_active>

    <sql>MERGE INTO IPTV.SYNNEX_ORDER USING dual ON (PHONE_NO='?')
WHEN MATCHED     THEN 
    UPDATE SET
    PURCHASE_DATE = (timestamp '1970-01-01 00:00:00 GMT' + numtodsinterval(?/1000, 'SECOND'))
	, PACKAGE_ID = 'WD0005'
    , STATUS_FLG = 'A'
    , CREATE_DATE = SYSDATE
    , SERVICE_ACT_DATE = null
WHEN NOT MATCHED THEN 
    INSERT (
    PURCHASE_DATE
	, PHONE_NO
	, PACKAGE_ID
    , STATUS_FLG
	, CREATE_DATE
	, SERVICE_ACT_DATE) 

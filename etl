        <jsScript_script>//Script here
var mac;
mac=upper(replace(載具MAC,':',''));
var phone_no;
var pad = "0000000000";


if(註冊手機 != null){
var v_p = num2str(str2num(註冊手機),'#0');

var phone_no = pad.substring(0, pad.length - v_p.length) + v_p;
}
else
{
  var phone_no='';
}
</jsScript_script>

    <connection>twbidb_pentaho</connection>
    <sql>select x.p_localdate,case when substr(ad_clip,1,1) in ('3','7') then 'ad-video' when substr(ad_clip,1,3) in ('Goo','AOL') then 'ad-video' else ad_id end "ad_id",
 ad_clip "ad_clip",cdata "cdata",platform "platform",count(*) cnt ,x.p_localhour,case when instr(x.program_id,'M') > 0 then substr(x.program_id,1,instr(x.program_id,'M')-1) else x.program_id end program_id
              from pentaho.row_ad3 x
             where 1 = 1
               AND (('${type}'='r' and  p_localdate=to_char((sysdate-3/24),'YYYY/MM/DD')
  		       and  p_localhour >= to_char((sysdate-3/24),'HH24'))
               OR ('${type}'='d' AND  p_localdate='${date}'))

-- and x.ad_clip like '%3869219%'
-- and p_localhour=11
--  AND  p_localdate='2017/11/24'
 -- and ad_clip like '%drama_pr%'
--and cdata='noAd'
--and platform='ios'
group by  x.p_localdate,case when substr(ad_clip,1,1) in ('3','7') then 'ad-video' when substr(ad_clip,1,3) in ('Goo','AOL')  then 'ad-video' else ad_id end  ,ad_clip,cdata ,platform,x.p_localhour,case when instr(x.program_id,'M') > 0 then substr(x.program_id,1,instr(x.program_id,'M')-1) else x.program_id end
order by p_localdate,p_localhour,platform,ad_clip,cdata
</sql>
    <limit>0</limit>
    <lookup/>
    <execute_each_row>N</execute_each_row>
    <variables_active>Y</variables_active>
    <lazy_conversion_active>N</lazy_conversion_active>
    <cached_row_meta_active>N</cached_row_meta_active>
    <attributes/>
    <cluster_schema/>
    <remotesteps>
      <input>
      </input>

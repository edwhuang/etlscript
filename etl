    <sql>select x.p_localdate, ad_id "ad_id" ,
 ad_clip "ad_clip",cdata "cdata",platform "platform",count(*) cnt ,x.p_localhour,case when instr(x.program_id,'M') > 0 then substr(x.program_id,1,instr(x.program_id,'M')-1) else x.program_id end program_id
              from pentaho.row_ad_stg x
             where 1 = 1
               AND p_localdate=to_char((sysdate-3/24),'YYYY/MM/DD')
  and  p_localhour >= to_char((sysdate-3/24),'HH24')
--  AND
-- p_localdate='2017/05/23'
--and cdata='noAd'
--and platform='ios'
group by  x.p_localdate,ad_id ,ad_clip,cdata ,platform,x.p_localhour,case when instr(x.program_id,'M') > 0 then substr(x.program_id,1,instr(x.program_id,'M')-1) else x.program_id end
</sql>

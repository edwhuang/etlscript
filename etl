  <step>
    <name>Database join</name>
    <type>DBJoin</type>
    <description/>
    <distribute>Y</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <connection>ClickHousre</connection>
    <rowlimit>0</rowlimit>
    <sql>SELECT 
    PACKAGE_CAT_ID1,'服務有效' FIRST_BUY_NO,'有效' IN_SERVICE,'有效' ANS_CODE1,PAY_TYPE,SOFTWARE_GROUP,COUNT(*) P_COUNT
FROM rdbms.temp_purchase_dtls
WHERE PACKAGE_CAT_ID1 in ('ALL','CHANNEL_A','VOD_CHANNEL_DELUX')
AND PAY_TYPE in ('信用卡','便利商店繳款','ATM')
AND SERVICE_START_DATE &lt;= ?
AND SERVICE_END_DATE >= ?
GROUP BY PACKAGE_CAT_ID1,PAY_TYPE,SOFTWARE_GROUP</sql>

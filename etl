b.end_client_ids,
b.start_purchase_ids,
b.end_purchase_ids

from pentaho.ans_purchase_detail a ,pentaho.ans_purchase_week b
where b.pk_no=a.pk_no
and week_for_year=to_char(sysdate-14,'YYYY-WW')
and b.package_id not in ('CHG003')

    <connection>twdevdb</connection>
    <sql>Select c.owner_phone,
       c.serial_id,
       e.package_cat_id1,
       e.package_id,
       e.description,
       max(d.end_date) end_date,
       max(e.package_cat1) package_cat1,
       nvl(f.pay_type,'NULL') pay_type
  from bsm_client_details d, bsm_client_mas c, bsm_package_mas e,bsm_purchase_mas f
 where c.serial_id(+) = d.serial_id
   and d.status_flg = 'P'
   and e.package_id = d.package_id
   and d.end_date > sysdate
   and f.pk_no (+) =d.src_pk_no
 group by c.owner_phone, c.serial_id, e.package_cat_id1,f.pay_type, e.package_id,
       e.description</sql>
    <limit>0</limit>

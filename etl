  <step>
    <name>讀入購買資料</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <connection>twbidb_125</connection>
    <sql>select to_char(trunc(c.purchase_date),'YYYY-MM-DD') PURCHASE_DATE,
         c.purchase_no,
         c.pay_type,
         c.client_id,
         c.package_id,
         d.package_cat_id1,
         d.package_cat1,
         d.ans_code1,
         to_char(trunc(c.service_start_date),'YYYY-MM-DD') service_start_date,
         to_char(trunc(c.service_end_date),'YYYY-MM-DD') service_end_date
         ,(
         select  '續購'
    from pentaho.ans_purchase_detail e
   where e.pay_type in (Select pay_type
                          from pentaho.ans_payment_mas
                         where pay_type_group = '客戶購買' or 
pay_type_group ='系統扣款')
     and e.status_flg='Z'
     and e.client_id=c.CLIENT_ID
     and e.purchase_no &lt; c.PURCHASE_NO
     and c.PURCHASE_DATE BETWEEN e.service_start_date
     and e.service_end_date
     and rownum &lt;=1
) in_service,
         (
         select  '後續購買'
    from pentaho.ans_purchase_detail e
   where e.pay_type in (Select pay_type
                          from pentaho.ans_payment_mas
                         where pay_type_group = '客戶購買' or 
pay_type_group ='系統扣款')
     and e.status_flg='Z'
     and e.client_id=c.CLIENT_ID
     and e.purchase_no > c.PURCHASE_NO
     and e.purchase_date BETWEEN c.service_start_date
     and c.service_end_date
     and rownum &lt;=1
) resume_service,
    to_char(trunc(stop_recurrent_date),'YYYY-MM-DD') stop_recurrent_date,
    current_recurrent_s,
    recurrent,
    pay_type,
    c.software_group
    from pentaho.ans_purchase_detail c, pentaho.ans_package_mas d
   where c.client_id like '2A%'
     and c.pay_type in (Select pay_type
                          from pentaho.ans_payment_mas
                         where  pay_type_group = '客戶購買' or
pay_type_group ='系統扣款'
)
     and d.package_id = c.package_id
     and c.status_flg='Z'
   --  and c.purchase_date >= sysdate -180
order by client_id,purchase_no</sql>
    <limit>0</limit>

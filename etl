var p_ad_id='';
if(ad_id == 'ad_p_b')
{
  if(indexOf(ad_clip,'logo') >= 0 ||indexOf(ad_clip,'vod') >= 0 || indexOf(ad_clip,'noad') >= 0 || indexOf(ad_clip,'_ts') >= 0|| indexOf(ad_clip,'_TS') >= 0)
{p_ad_id='ad_other';}
else
{p_ad_id= 'ad_p_b';}
}
else
{ p_ad_id=ad_id;}


//Script here

var ad_clip2=ad_clip;
var ad_clip3=ad_clip;
if(platform =="|web"){var platform_2 = 'web';}
else if (platform =="|m-web"){var platform_2 = 'm-web';}
else if (indexOf(platform,'|tv') >=0 ){platform_2 = 'tv';}
else if(!(platform =="web" || platform=="m-web" || platform=="tv"))
{platform_2 = 'mobile';}
else
{platform_2= platform;}
if(indexOf(ad_clip,'IOS_') >= 0||indexOf(platform,'iPhone') >= 0||indexOf(platform,'iPad') >= 0)
{
  platform_2='ios';
  ad_clip2 =replace(ad_clip,'IOS_','');
 if(indexOf(ad_clip,'IOS_') >= 0){
  ad_clip2 =replace(ad_clip,'IOS_','');
  if(indexOf(ad_clip2,'|')>=0){
      ad_clip2 =substr(ad_clip2,0,indexOf(ad_clip2,'|'));
   }
} 
}


if(ad_clip == '793604647')
{
    platform_2='embed';
}

if(ad_clip == '7936064638')
{
    platform_2='embed';
}


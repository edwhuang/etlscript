    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <connection>twdevdb</connection>
    <sql>SELECT mas_No,
       y.SUPPLY_ID,
       y.PAY_SUPPLY_ID,
       x.PROGRAM_ID,
       z.PACKAGE_ID,
       nvl(z.AMOUNT,0) AMOUNT,
	   ROUND(NVL(z.AMOUNT,0)/1.05) NET_AMOUNT,
       NVL(z.AMOUNT,0) - ROUND(NVL(z.AMOUNT,0)/1.05) TAX_AMOUNT,
       y.auto_exten_service
  FROM BSM_COUPON_MAS x, bsm_coupon_prog_mas y, BSM_COUPON_DETAILS z
 WHERE     y.cup_program_id = x.program_id
       AND z.MAS_PK_NO = x.PK_NO
</sql>

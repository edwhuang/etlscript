      <schema_name/>
    </partitioning>
    <connection>twbidb_pentaho</connection>
    <sql>select to_char(local_time,'YYYYMMDD') "p_date",case when substr(ad_clip,1,1) in ('3','7','4') then 'ad-video' when substr(ad_clip,1,2) in ('hi') then 'ad-video' 
when (ad_clip like 'adx%' or ad_clip like 'adsense%' ) then 'ad-video'  else ad_id end "ad_id",
 ad_clip "ad_clip",cdata "cdata",platform "platform",count(*) "cnt",case when instr(program_id,'M') > 0 then substr(program_id,1,instr(program_id,'M') - 1) else program_id end "program_id"
              from pentaho.row_ad3
           where 1 = 1
               AND p_localdate =replace('${start_date}','-','/')
              --  AND p_localdate ='2017/10/03'
			 --   AND ad_clip like 'adx%'
            group by to_char(local_time,'YYYYMMDD'),ad_id,ad_clip,cdata,platform,case when instr(program_id,'M') > 0 then substr(program_id,1,instr(program_id,'M') - 1) else program_id end </sql>
    <limit>0</limit>

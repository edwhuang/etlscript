    <jsScripts>
      <jsScript>
        <jsScript_type>0</jsScript_type>
        <jsScript_name>Script 1</jsScript_name>
        <jsScript_script>//Script here
var _yymm=10901;
var _year ='108';
var _month = '12';
var p_year=108;
var p_month=12;
var service_name='cht';
var nohn_aauid = '0';
var p_optw2 = '0';
var p_yyymm ='2019';

try{
_yymm=yymm;
_year =substr(_yymm,0,3);
_month = substr(_yymm,3,2);
p_year=str2num(_year);
p_month=str2num(_month);
service_name=substr(filename,0,7);
nohn_aauid = replace(aauid,'HN','');
p_optw2 = replace(remark,'76  100','');
p_optw2 = replace( p_optw2,' 2','');
if( indexOf(filename,'110') >0 ) {
p_yyymm =substr(filename,indexOf(filename,'110'),5); 
}
else
{
p_yyymm =substr(filename,indexOf(filename,'10'),5); 
}
}
catch(e)
{
}</jsScript_script>
      </jsScript>
    </jsScripts>

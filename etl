       a.vendor_id,
       to_char(a.purchase_date, 'YYYY/MM/DD') purchase_date_s,
       c.status_flg service_status,
       a.response_code,
       a.pay_pk_no,
       a.tax_inv_no,
       a.tax_inv_date,
       a.tax_flg,
       a.card_no,
       a.card_type,
       a.card_expiry,
       a.promo_code,
       0 fa_ref_item_pk_no,
       a.lipay_tx_number

FROM bsm_purchase_mas a,bsm_purchase_item b,bsm_client_details c,bsm_client_mas d,web_registered e
 WHERE  1=1 
and a.status_flg in ('Z','C')
 and (a.report_type='P' or a.tax_inv_no is not null) 
and b.mas_pk_no = a.pk_no
and c.src_item_pk_no(+) =b.pk_no
and d.serial_id(+)=a.serial_id
and e.accountid(+)=a.serial_id
and a.serial_id not like 'F%'
and ( a.mas_date >= to_date(to_char(sysdate-10,'YYYYMM')||'01','YYYYMMDD')
 or a.purchase_date >= to_date(to_char(sysdate-10,'YYYYMM')||'01','YYYYMMDD'))





</sql>

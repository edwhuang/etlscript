  <step>
    <name>twdevdb_註冊資料</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <connection>twdevdb</connection>
    <sql>with cte as (
    select to_char(a.activate_date,'yyyy/mm/dd') activation_date,a.device_id real_mac_address,a.client_id mac_address,b.owner_phone,nvl(c.software_group,'NULL') software,
           c.software_desc softwaregroup_description,
           c.ref1 TV,
           b.owner_id CUST_ID,tgc_util.GET_CUST_NAME(b.owner_id) cust_name,b.status_flg,
           c.ref1 TV_cust_name,
           (select count(distinct x.pk_no)
                    from bsm_purchase_mas x, bsm_purchase_item y
                   where x.pk_no = y.mas_pk_no
                     and x.serial_id = b.mac_address
                     and y.device_id = a.device_id
                     and x.pay_type = '兌換券'
                     and x.status_flg in ('P', 'Z')) counpon_count,
           to_char(a.activate_date,'yyyy-mm') activation_month,
           decode(c.report_type,'N','N',b.report_type) report_type
    from bsm_client_device_list a
    join bsm_client_mas b
    on a.client_id=b.mac_address
    left join mfg_softwaregroup_mas c
    on c.software_group=get_software_group(b.mac_address,a.device_id)
    WHERE 1=1
      and b.status_flg in ('A','W')
 and to_char(a.activate_date,'yyyy/mm/dd') >= to_char(sysdate-15,'YYYY/MM/DD')
    )
    select cte.*,case when row_number()over(partition by substr(nvl(real_mac_address,mac_address),1,12) order by activation_date desc)=1 then 'YES'
                 else '重覆' end real_mac_jud
    from cte
    where 1=1
    order by 1,2,3</sql>
    <limit>0</limit>
    <lookup/>
    <execute_each_row>N</execute_each_row>
    <variables_active>N</variables_active>

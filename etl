    <sql>select
 to_number(to_char(
to_date(p_date,'YYYY/MM/DD'),'YYYY')) p_year,
 to_number(to_char(
to_date(p_date,'YYYY/MM/DD'),'MM')) p_month,
 to_number(to_char(
to_date(p_date,'YYYY/MM/DD'),'DD')) p_day,
to_char(
to_date(p_date,'YYYY/MM/DD'),'Day') p_week,

p_date,
service_name,
total_watch_time,
total_watch_time_hours,
online_max_time_stamp,
online_max,
bitrate_max,
ip_count,
avg_watch_time,
volume_mb,
access_count,
bitrate_max_time_stamp,
program_name,
program_type,
program_id,
category_id,
category_name,
content_type,
content_id,
content_title,
episode_no,
provider,
case
  when instr(service_name,'free') > 0 then
    '免費'
  else
    '付費'
  end 
free,
cat1
from iptv.cdn_programinfo_view
where content_type is not null
and time='day'

</sql>

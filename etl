var _category_id = CATEGORY_ID;
var _category_name = CATEGORY_NAME;
var _content_type= CONTENT_TYPE


if(_category_id == 'MOVIE') {
   _category_name = '隨選電影';
}

if(_category_id == 'HD') {
   _category_id	  = 'MOVIE';
   _category_name = '隨選電影';
}

if(_category_id == 'WEEKEND_CINEMA') {
   _category_id	  = 'MOVIE';
   _category_name = '隨選電影';
}

if(_category_id == 'MOVIE') {
   _category_name = '隨選電影';
}

if(_category_id == 'TOPIC') {
   _category_name = '免費電影';
}

if(_category_id == 'ENT') {
   _category_name = '綜藝館';
}

if(_category_id == 'CHANNEL') {
   _category_id	  = 'LIVE';
   _category_name = 'channel';
   _content_type = 'live';
}

if(_category_id == 'OTHERS') {
   _category_id	  = 'HOUSE_VIDEO';
   _category_name = 'channel';
   _content_type = 'house_video';
}

</jsScript_script>
      </jsScript>
    </jsScripts>

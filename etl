      </output>
    </remotesteps>
    <GUI>
      <xloc>112</xloc>
      <yloc>128</yloc>
      <draw>Y</draw>
    </GUI>
  </step>
  <step>
    <name>Row_import 2</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <connection>twbidb_pentaho</connection>
    <sql>select x.p_localdate,case when substr(ad_clip,1,1) in ('3','7') then 'ad-video' else ad_id end "ad_id",
 ad_clip "ad_clip",cdata "cdata",platform "platform",count(*) cnt ,x.p_localhour,case when instr(x.program_id,'M') > 0 then substr(x.program_id,1,instr(x.program_id,'M')-1) else x.program_id end program_id
              from pentaho.row_ad_4gtv x
             where 1 = 1
               AND p_localdate=to_char((sysdate-24/24),'YYYY/MM/DD')
 --and p_localhour >= to_char((sysdate-1/24),'HH24')
  AND p_localdate='2017/03/21'
group by  x.p_localdate,case when substr(ad_clip,1,1) in ('3','7') then 'ad-video' else ad_id end ,ad_clip,cdata ,platform,x.p_localhour,case when instr(x.program_id,'M') > 0 then substr(x.program_id,1,instr(x.program_id,'M')-1) else x.program_id end
</sql>
    <limit>0</limit>

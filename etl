      <from>讀取頻道日報表資料</from>
      <to>寫入頻道每日日報表Oracle</to>
      <enabled>Y</enabled>
    </hop>
    <hop>
      <from>寫入頻道每日日報表Oracle</from>
      <to>寫入頻道每日日報表ClickHouse</to>
      <enabled>Y</enabled>
    </hop>
    <hop>
      <from>刪除ClickHouse 頻道日報表資料</from>
      <to>刪除舊的頻道日報表</to>
      <enabled>Y</enabled>
    </hop>
    <hop>
      <from>刪除ClickHouseOLAP資料</from>
      <to>讀取頻道日報表</to>
      <enabled>Y</enabled>
    </hop>
    <hop>
      <from>讀取頻道日報表</from>
      <to>Table output</to>
      <enabled>Y</enabled>
    </hop>
    <hop>
      <from>讀取頻道日報表資料 2</from>
      <to>寫入頻道每日日報表ClickHouse 2</to>
      <enabled>Y</enabled>
    </hop>
    <hop>
      <from>刪除ClickHouse 頻道日報表資料 2</from>
      <to>讀取頻道日報表資料 2</to>
      <enabled>Y</enabled>
    </hop>
  </order>

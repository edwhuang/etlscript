        <jsScript_script>//Script here

var p_device_type = Field2;
var p_f3=Field3;
var p_channel = "";
var p_vod_asset = "";
var p_dt_s =p_date+' '+p_time;
var p_dt=str2date(p_dt_s,'yyyy/MM/dd hh:mm:ss');
p_dt.setHours(p_dt.getHours()+8);
var p_dt2 =date2str(p_dt,'yyyy/MM/dd');
if( indexOf(p_f3,'#') >= 0 )
{
  p_channel = substr(p_f3,0,indexOf(p_f3,"#"));
  p_vod_asset = substr(p_f3,indexOf(p_f3,"#")+1);

}
else if( indexOf(p_f3,"vod") >=0 )
{
  p_vod_asset = p_f3;
}
else 
{
 p_channel= p_f3;
}


var p_program_id=substr(p_vod_asset,0,indexOf(p_vod_asset,'-')+7);

var p_date_str=replace(p_dt2,'-','/');

var p_program_id2="";
if (p_program_id=="")
{
	p_program_id2=p_channel;
}
else
{
  p_program_id2=p_program_id;
}
</jsScript_script>

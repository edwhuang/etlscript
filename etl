    <connection>twbidb125_pentaho</connection>
    <sql>Select 'drama' content_type,sum(b.access_count) access_count from iptv.content_meta a ,pentaho.ans_cdn_daily_report b,pentaho.ans_cdn_service_name c
where b.program_id=a.program_id
and b.p_date >= to_char((to_date('20171201','YYYYMMDD')+trunc((sysdate-to_date('20171201','YYYYMMDD'))/7)*7)-7,'YYYY/MM/DD')
and b.p_date &lt; to_char((to_date('20171201','YYYYMMDD')+trunc((sysdate-to_date('20171201','YYYYMMDD'))/7)*7),'YYYY/MM/DD')
and c.service_name = b.service_name
and c.cal_type = '免費'
and b.service_name in ('litvfreepc','litvfreemobile')

union all 
Select 'show' content_type,sum(b.access_count) access_count from iptv.content_meta a ,pentaho.ans_cdn_daily_report b,pentaho.ans_cdn_service_name c
where b.program_id=a.program_id
and b.p_date >= to_char((to_date('20171201','YYYYMMDD')+trunc((sysdate-to_date('20171201','YYYYMMDD'))/7)*7)-7,'YYYY/MM/DD')
and b.p_date &lt; to_char((to_date('20171201','YYYYMMDD')+trunc((sysdate-to_date('20171201','YYYYMMDD'))/7)*7),'YYYY/MM/DD')
and c.service_name = b.service_name
and c.cal_type = '免費'
and b.service_name in ('litvfreepc','litvfreemobile')
</sql>

    <sql>Select a.mas_no,a.mas_date,a.status_flg,
       ISSUE_TYPE,
       a.PK_NO,
       b.pk_no PK_NO_1,
       b.REFUND_AMT*-1 REFUND_AMT_1,
       a.client_id,
       to_char(mas_date, 'YYYY/MM/DD') PURCHASE_DATE_S,
       'SYSTEM' SOFTWARE_GROUP,
       a.PURCHASE_NO,
       b.REFUND_NET_AMT*-1 REFUND_NET_AMT_1,
       b.REFUND_TAX*-1 REFUND_TAX_1,
       b.NEW_START_DATE,
       b.NEW_END_DATE,
       b.new_package_id package_id
  from bsm_issue_mas a, bsm_issue_item b
 where a.issue_type in ('OTHER_REFUND')
   and b.mas_pk_no = a.pk_no
</sql>

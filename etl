      <jsScript>
        <jsScript_type>0</jsScript_type>
        <jsScript_name>Script 1</jsScript_name>
        <jsScript_script>//Script here

var a=getVariable("current_date", "");
P_DATE=replace(a,"-","/");
SNAME=getVariable("service_name", "");
LOG_TYPE=getVariable("log_type","");
var _Program_Name;
_Program_Name = Program_Name;

var p_M = indexOf(_Program_Name,"M",-1);

var p_Program_id;
if(p_M &lt;= 0 ) {
  _Program_name = _Program_Name;
} else { 
   _Program_Name = substr(_Program_Name,1,p_M - 1);
}

p_M = indexOf(_Program_Name,"P",-1);
if(p_M &lt;= 0 ) {
  _Program_Name = _Program_Name;
} else { 
  _Program_Name = substr(_Program_Name,1,p_M - 1);
}

p_M = indexOf(_Program_Name,"B",-1);
if(p_M &lt;= 0 ) {
  _Program_Name = _Program_Name;
} else { 
  _Program_Name = substr(_Program_Name,1,p_M - 1);
}

if(indexOf(_Program_Name,"vod") >0){
p_Program_id=substr(_Program_Name,indexOf(_Program_Name,"vod"));
p_Program_id=substr(p_Program_id,0,indexOf(p_Program_id,"-")+7);
}
else
{p_Program_id=_Program_Name;}


PROGRAM_ID=p_Program_id;


  

</jsScript_script>
      </jsScript>
    </jsScripts>

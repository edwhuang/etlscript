    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <connection>twodb</connection>
    <sql>select C.OWNER_PHONE "用戶電話號碼"
  , A.COUPON_SERIAL_NO "COUPON序號" 
  -- , E.ELM_INV_STATUS "全國電子交易狀態" 
  -- , E.ELM_INV_ID "全國電子交易序號" 
  -- , E.ELM_INV_SALETIME "全國電子交易時間" 
  -- , E.ELM_INV_CHECKED_FLG "全國電子檢查狀態"   
  from IPTV.BSM_COUPON_MAS a 
  join IPTV.BSM_CLIENT_MAS c
  on A.SERIAL_ID = C.SERIAL_ID
  left join IPTV.ELM_INVOICE E 
  on A.COUPON_SERIAL_NO = E.ELM_INV_CUP_SERIAL_NO 
  where A.SRC_NO = 'CB00001565'   -- 全國電子一年豪華組合加贈A2V優惠專案coupon batch 2017/11/09
  -- A.PROGRAM_ID = 'ELFLI001'   -- 全國電子一年豪華組合加贈A2V優惠專案coupon program   
  and A.STATUS_FLG = 'Z'        
  and trunc(A.REGISTER_DATE) &lt;= trunc(sysdate - 1)        
  and (E.ELM_INV_CHECKED_FLG is null)       
  order by A.COUPON_SERIAL_NO, E.ELM_INV_ID desc</sql>
    <limit>0</limit>

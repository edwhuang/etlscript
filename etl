    <sql>SELECT PURCHASE_DATE,
       PACKAGE_CAT_ID1,
       FIRST_BUY_NO,
       IN_SERVICE,
       ANS_CODE1,
       PAY_TYPE,
       SOFTWARE_GROUP,
       COUNT(*) P_COUNT
  FROM rdbms.temp_purchase_dtls
 WHERE PACKAGE_CAT_ID1 in ('ALL', 'CHANNEL_A', 'VOD_CHANNEL_DELUX')
   AND PAY_TYPE in
       ('信用卡', '便利商店繳款', 'ATM', '信用卡二次扣款', 'IOS二次扣款','IOS','GOOGLEPLAY')
 GROUP BY PURCHASE_DATE,
       PACKAGE_CAT_ID1,
       FIRST_BUY_NO,
       IN_SERVICE,
       ANS_CODE1,
       PAY_TYPE,
       SOFTWARE_GROUP
 ORDER BY PURCHASE_DATE,
          PACKAGE_CAT_ID1,
          FIRST_BUY_NO,
          IN_SERVICE,
          ANS_CODE1
</sql>

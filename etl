  <step>
    <name>Table input</name>
    <type>TableInput</type>
    <description/>
    <distribute>N</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <connection>twdevdb_pentaho</connection>
    <sql> 
select case
         when b.platform = 'web' then
          'web'
         when platform = 'm-web' then
          'm-web'
         else
          'mobile'
       end platform,
       to_char(b.local_time,'YYYY/MM/DD'),
       nvl(a.des, b.ad_clip) content_type,
       b.cdata,
       b.asset_id,
       d.category_id,d.category_name,d.content_type,d.content_id,d.content_title,d.episode_no,d.provider,
       count(*) cnt
  from row_ad b, iptv.ad_clip_map a,(select a.asset_id,max(distinct UPPER(a.content_type)) category_id,
                        decode(max(a.content_type),
                               'movie',
                               '隨選電影',
                               'comic',
                               '隨選動漫',
                               'drama',
                               '隨選戲劇',
                               'live',
                               '直播頻道') category_name,
                        max(a.content_type) content_type,
                        max(a.content_id) content_id,
                        max(a.content_title) content_title,
                        max(a.episode_no) episode_no,
                       max(nvl((select  b.cname from  iptv.cms_provider b where b.ename=a.provider
         or to_char(b.sid) = a.provider),a.provider))  provider
          from iptv.ccc_program_asset a, iptv.cms_provider b
        group by asset_id) d
where a.ad_clip(+) = b.ad_clip
and d.asset_id(+)=b.asset_id
  group by b.cdata,b.asset_id,to_char(b.local_time,'YYYY/MM/DD'),platform,nvl(a.des, b.ad_clip), d.category_id,d.category_name,d.content_type,d.content_id,d.content_title,d.episode_no,d.provider</sql>
    <limit>0</limit>

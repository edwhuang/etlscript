var p_software_group =SOFTWARE_GROUP;
if(p_software_group==null){
  p_software_group="SYSTEM";
}

if(p_software_group=="")
{
  p_software_group="SYSTEM";
}

if(PAY_TYPE=='IOS')
{
  p_software_group="LTIOS03";
}

var v_net_amt = AMOUNT_1;
var v_tax_amt = 0;

if(TAX_INV_NO!=null){
  v_net_amt=ceil(AMOUNT_1/1.05);
  v_tax_amt=AMOUNT_1-v_net_amt;
}

if(PAY_TYPE=='REMIT')
{
   PAY_TYPE='便利商店繳款';
}

if(PAY_TYPE=='CREDIT')
{
   PAY_TYPE='信用卡';
}</jsScript_script>
